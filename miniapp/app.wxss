
@import "colorui/main.wxss";
@import "colorui/icon.wxss";


.van-picker-column__item--selected{
  font-size: 1rem;
}

.svg_icon {
  display: inline-flex;
  width: 2em;
  height: 2em;
  overflow: hidden;
}

.svg_icon-inner {
  width: calc(2em + 0.5 * (1rem - 16px));
  height: calc(2em + 0.5 * (1rem - 16px));
  /* transform: translateY(-1em); */
  filter: drop-shadow(0 1em 0 currentColor);
}

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;

}

/* text  animation */
