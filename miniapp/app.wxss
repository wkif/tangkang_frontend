@import "colorui/main.wxss";
@import "colorui/icon.wxss";



.svg_icon {
  display: inline-flex;
  width: 2em;
  height: 2em;
  overflow: hidden;
}

.svg_icon-inner {
  width: 2em;
  height: 2em;
  /* transform: translateY(-1em); */
  filter: drop-shadow(0 1em 0 currentColor);
}

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;

}